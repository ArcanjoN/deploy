{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "commands": [
        "./mvnw dependency:resolve"
      ],
      "caches": [
        {
          "directory": "/root/.m2/repository",
          "type": "shared"
        }
      ]
    },
    "build": {
      "inputs": [
        {
          "step": "install"
        }
      ],
      "commands": [
        "./mvnw package -DskipTests"
      ]
    }
  },
  "deploy": {
    "base": {
      "image": "eclipse-temurin:17"
    },
    "inputs": [
      {
        "step": "build",
        "include": ["projectjavaJL/target/*.jar"]
      }
    ],
    "startCommand": "java -jar projectjavaJL/target/NOME_DO_SEU_JAR.jar"
  }
}
